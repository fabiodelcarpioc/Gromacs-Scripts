sc-coul                  = no
separate-dhdl-file       = yes
dhdl-derivatives         = yes
dh_hist_size             = 0
dh_hist_spacing          = 0.1

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
